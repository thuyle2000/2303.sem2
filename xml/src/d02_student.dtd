<!ELEMENT StudentList (Student+)>
<!ELEMENT Student (name, gender, dob, email, address)>
<!ELEMENT name (#PCDATA)>
<!ELEMENT gender (#PCDATA)>
<!ELEMENT dob (#PCDATA)>
<!ELEMENT email (#PCDATA)>
<!ELEMENT address (#PCDATA)>
<!ATTLIST Student id ID #REQUIRED active (true|false) #REQUIRED >

